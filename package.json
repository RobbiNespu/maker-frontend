{
  "name": "exam-maker-frontend",
  "version": "1.0.0",
  "description": "create, edit and share exams",
  "keywords": [
    "exam",
    "simulator"
  ],
  "author": {
    "name": "benjaminadk",
    "email": "jsonexamsimulator@gmail.com",
    "url": "https://github.com/exam-simulator"
  },
  "homepage": "https://github.com/exam-simulator/maker-frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/exam-simulator/maker-frontend.git"
  },
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "dev": "next -p 8080",
    "build": "next build",
    "start": "next start -p $PORT",
    "heroku-postbuild": "next build"
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.1.0"
  },
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.4.13",
    "apollo-link": "^1.2.8",
    "apollo-link-context": "^1.0.14",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-ws": "^1.0.14",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.10.0",
    "date-fns": "^2.0.0-alpha.27",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "next": "^8.0.3",
    "next-plugin-styled-icons": "^7.0.0",
    "next-with-apollo": "^3.3.2",
    "nprogress": "^0.2.0",
    "polished": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.3",
    "react-google-login": "^5.0.2",
    "styled-components": "^4.1.3",
    "styled-icons": "^7.2.0",
    "subscriptions-transport-ws": "^0.9.15",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "babel-jest": "^24.3.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.3.1",
    "jest-transform-graphql": "^2.1.0",
    "waait": "^1.0.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.next/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
